{
  "name": "factorio-tools-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:test": "prettier --list-different \"./projects/**/*.{ts,html,scss,json}\"",
    "format:write": "prettier --write \"./projects/**/*.{ts,html,scss,json}\"",
    "analyze": "ng build --stats-json --output-hashing none --named-chunks && esbuild-visualizer --template treemap --metadata dist/blueprint/stats.json --filename dist/blueprint/analyze/index.html && http-server -o -c-1 ./dist/blueprint/analyze/",
    "analyze:sme": "ng build --source-map --output-hashing none --named-chunks && source-map-explorer dist/blueprint/browser/*.js --html dist/blueprint/sme/index.html && http-server -o -c-1 ./dist/blueprint/sme/",
    "analyze:deps": "npm-run-all analyze:deps:*",
    "analyze:deps:all": "madge projects/blueprint/src/main.ts --ts-config tsconfig.json --image ./deps/blueprint/_all.png",
    "analyze:deps:ui": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/blueprint/ui.png projects/blueprint/src/app/ui/",
    "analyze:deps:pattern": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/blueprint/pattern.png projects/blueprint/src/app/pattern/",
    "analyze:deps:feature": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/blueprint/feature.png projects/blueprint/src/app/feature/",
    "analyze:deps:layout": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/blueprint/layout.png projects/blueprint/src/app/layout/",
    "analyze:deps:core": "madge  --extensions ts --ts-config tsconfig.json --image ./deps/blueprint/core.png projects/blueprint/src/app/core/",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@primeng/themes": "^19.0.5",
    "pako": "^2.1.0",
    "primeng": "^19.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.10.5",
    "@types/pako": "^2.0.3",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "esbuild-visualizer": "^0.7.0",
    "eslint": "^9.16.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-boundaries": "^5.0.1",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.18.0"
  }
}
